#!/bin/bash

couchHost="http://jfcenterprises.cloudant.com"
db="hive-sensor-log"
designDoc="_design/SensorLog"
view="by-hive-sensor"
listfunc="dump"

tf=$(mktemp)
echo curl ${couchHost}/${db}/${designDoc}/_list/${listfunc}/${view} 
curl ${couchHost}/${db}/${designDoc}/_list/${listfunc}/${view} 2> /dev/null > $tf
cp $tf dump.csv



